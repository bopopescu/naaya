	border-left: 1px solid #4E8EC2;
}

.border_tabs {
	border-bottom: 1px solid #4E8EC2;
	height: 22px;
	position: relative;
}
.tabs_slot {
	position: relative;
	top: 0px;
}

.tabs_slot2 {
	position: absolute;
	top: 0px;
}
.tab {
	border: 1px solid #4E8EC2;
	height: 22px;	
	float: left;

