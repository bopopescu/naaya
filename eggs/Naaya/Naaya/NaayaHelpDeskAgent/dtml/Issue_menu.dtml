<dtml-let hasRoleHelpDeskAdministrator="isHelpDeskAdministrator()" hasRoleHelpConsultant="isHelpDeskConsultant()" issueOwner="hasRoleHelpDeskAdministrator or (hasRoleHelpConsultant and (consultant==AUTHENTICATED_USER.getUserName()))">

<br>
<table border="0" cellspacing="0" cellpadding="0" width="100%">
    <tr>
        <td>
            <table border="0" cellspacing="0" cellpadding="0" width="100%">
                <tr>
                    <td nowrap="true"><img src="<dtml-var icon>" border="0" alt="<dtml-var meta_type html_quote>">&nbsp;<a class="title2" href=""><dtml-var subject html_quote size=50></a></td>
                    <td width="100%">&nbsp;</td>
                    <td class="<dtml-if "pagetab==0">tabon<dtml-else>taboff</dtml-if>" align="left" valign="top"><img src="misc_/HelpDeskAgent/menu_top_left" border="0"></td>
                    <td class="<dtml-if "pagetab==0">tabon<dtml-else>taboff</dtml-if>" nowrap="true"><a class="tablink" href="<dtml-var "absolute_url(0)">/index_html" title="View Issue">View</a></td>
                    <td class="<dtml-if "pagetab==0">tabon<dtml-else>taboff</dtml-if>" align="right" valign="top"><img src="misc_/HelpDeskAgent/menu_top_right" border="0"></td>
                    <td><img src="misc_/HelpDeskAgent/spacer" border="0" height="3" width="3"></td>
                    <dtml-if issueOwner>
                        <td class="<dtml-if "pagetab==1">tabon<dtml-else>taboff</dtml-if>" align="left" valign="top"><img src="misc_/HelpDeskAgent/menu_top_left" border="0"></td>
                        <td class="<dtml-if "pagetab==1">tabon<dtml-else>taboff</dtml-if>" nowrap="true"><a class="tablink" href="<dtml-var "absolute_url(0)">/edit_user_html" title="Edit Issue Details">Edit</a></td>
                        <td class="<dtml-if "pagetab==1">tabon<dtml-else>taboff</dtml-if>" align="right" valign="top"><img src="misc_/HelpDeskAgent/menu_top_right" border="0"></td>
                        <td><img src="misc_/HelpDeskAgent/spacer" border="0" height="3" width="3"></td>
                    </dtml-if>
                    <td class="<dtml-if "pagetab==2">tabon<dtml-else>taboff</dtml-if>" align="left" valign="top"><img src="misc_/HelpDeskAgent/menu_top_left" border="0"></td>
                    <td class="<dtml-if "pagetab==2">tabon<dtml-else>taboff</dtml-if>" nowrap="true"><a class="tablink" href="<dtml-var "absolute_url(0)">/comments_user_html" title="Add Comments to current issue">Add Comment</a></td>
                    <td class="<dtml-if "pagetab==2">tabon<dtml-else>taboff</dtml-if>" align="right" valign="top"><img src="misc_/HelpDeskAgent/menu_top_right" border="0"></td>
                    <dtml-if issueOwner>
                        <td><img src="misc_/HelpDeskAgent/spacer" border="0" height="3" width="3"></td>
                        <td class="<dtml-if "pagetab==3">tabon<dtml-else>taboff</dtml-if>" align="left" valign="top"><img src="misc_/HelpDeskAgent/menu_top_left" border="0"></td>
                        <td class="<dtml-if "pagetab==3">tabon<dtml-else>taboff</dtml-if>" nowrap="true"><a class="tablink" href="<dtml-var "absolute_url(0)">/history_user_html" title="Issue History">History</a></td>
                        <td class="<dtml-if "pagetab==3">tabon<dtml-else>taboff</dtml-if>" align="right" valign="top"><img src="misc_/HelpDeskAgent/menu_top_right" border="0"></td>
                    </dtml-if>
                </tr>
            </table>
        </td>
    </tr>
    <tr><td class="tabon"><img src="misc_/HelpDeskAgent/spacer" border="0" height="3" width="3"></td></tr>
</table>

</dtml-let>
